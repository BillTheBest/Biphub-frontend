{
  "extends": "vue",
  "rules": {
    "func-names": ["error", "never"],
    "no-tabs": 0,
    "no-param-reassign": ["off"],
    "no-extra-semi": "error",
    "semi": ["error", "never"],
    "consistent-return": ["off"],
    "no-console": [1],
    "comma-dangle": ["error", {
      "arrays": "only-multiline",
      "objects": "only-multiline",
      "imports": "only-multiline",
      "exports": "only-multiline",
      "functions": "only-multiline"
    }]
  }
}

